!function(last_passport_request_index,lng_passport_request){function process_alert(xhr_response){res=JSON.parse(xhr_response),res.result&&res.show&&res.text?(st_node=document.createElement("link"),st_node.setAttribute("rel","stylesheet"),st_node.setAttribute("href","/css/alert.css"),document.head.appendChild(st_node),obj=document.getElementById("alert_cnt"),alert=document.createElement("div"),alert.id="alert",alert_panel=document.createElement("div"),alert_panel.style.display="none",alert_panel.id="alert-panel",alert_panel_cnt=document.createElement("div"),alert_panel_cnt.id="alert-panel-cnt",alert_panel.appendChild(alert),alert_panel_cnt.innerHTML=res.text,obj.appendChild(alert),alert_panel.appendChild(alert_panel_cnt),obj.insertBefore(alert_panel,alert),res.btn_text&&(alert_panel_btn=document.createElement("a"),alert_panel_btn.id="alert-panel-btn",alert_panel_btn.className="hbutton",alert_panel_btn.style.textAlign="center",alert_panel_btn.style.paddingLeft=0,alert_panel_btn.innerHTML=res.btn_text,alert_panel_btn.setAttribute("href",res.btn_url),alert_panel_btn.setAttribute("target","_blank"),alert_panel_cnt.appendChild(alert_panel_btn)),setCookie("talert",xhr_response,864e3,"/"),add_event(document.getElementById("alert-panel-btn"),"click",(function(event){setNotifyTimeout(3),setCookie("talert","",0,"/"),document.getElementById("alert_cnt").style.display="none"})),add_event(document,"click",(function(event){document.getElementById("alert")!=event.target&&document.getElementById("alert-panel")!=event.target&&document.getElementById("alert-panel")!=event.target.parentNode&&document.getElementById("alert-panel")!=event.target.parentNode.parentNode&&(document.getElementById("alert-panel").style.display="none")})),add_event(document.getElementById("alert"),"click",(function(){document.getElementById("alert-panel").style.display="block"==document.getElementById("alert-panel").style.display?"none":"block"}))):setTimeout((function(){setNotifyTimeout(30)}),1e3)}function setNotifyTimeout(days){setCookie("tpass",last_passport_request_index,24*days*60*60,"/")}function add_event(obj,evname,listener){obj.addEventListener?obj.addEventListener(evname,listener,!1):obj.attachEvent("on"+evname,listener)}function setCookie(name,value,time_add,path,domain,secure){if(expires="",time_add){var now=new Date;now.setTime(now.getTime()+1e3*time_add),expires=now}var curCookie=name+"="+escape(value)+(expires?"; expires="+expires.toUTCString():"")+(path?"; path="+path:"")+(domain?"; domain="+domain:"")+(secure?"; secure":"");document.cookie=curCookie}function getCookie(name){var prefix=name+"=",cookieStartIndex=document.cookie.indexOf(prefix);if(-1==cookieStartIndex)return"";var cookieEndIndex=document.cookie.indexOf(";",cookieStartIndex+prefix.length);return-1==cookieEndIndex&&(cookieEndIndex=document.cookie.length),unescape(document.cookie.substring(cookieStartIndex+prefix.length,cookieEndIndex))}setTimeout((function(){if(void 0===last_passport_request_index&&(last_passport_request_index=0),void 0===lng_passport_request&&(lng_passport_request="ua"),last=getCookie("tpass"),alert=getCookie("talert"),alert||!last||last<last_passport_request_index)if(alert)process_alert(alert);else{var xhr=new XMLHttpRequest;xhr.withCredentials=!0,xhr.open("GET","https://passport.meta.ua/test_user_defense.php?lng="+lng_passport_request),xhr.responseType="text",xhr.send(),xhr.onload=function(){xhr.readyState===xhr.DONE&&(200===xhr.status?(process_alert(xhr.response),setNotifyTimeout(3)):setNotifyTimeout(1))}}}),500)}(last_passport_request_index,lng_passport_request);